{
    "name": "vscode-xyplorer",
    "displayName": "XYplorer",
    "description": "XYplorer language support for VS Code",
    "version": "0.2.1",
    "publisher": "Otiel",
    "icon": "icon.png",
    "homepage": "https://github.com/Otiel/vscode-xyplorer",
    "repository": {
        "type": "git",
        "url": "https://github.com/Otiel/vscode-xyplorer.git"
    },
    "bugs": {
        "url": "https://github.com/Otiel/vscode-xyplorer/issues"
    },
    "license": "See LICENSE file",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "xys",
            "aliases": ["XYplorer", "xys"],
            "extensions": [".xys",".xyi"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "xys",
            "scopeName": "source.xys",
            "path": "./syntaxes/xys.tmLanguage"
        }]
    },
    "galleryBanner": {
        "color": "#FE9900",
        "theme": "light"
    }
}